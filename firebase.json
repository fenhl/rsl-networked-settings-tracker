{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "public": "public",
    "rewrites": [
      {
        "source": "/horiz/**",
        "destination": "/rsl-horizontal-tracker.html"
      },
      {
        "source": "/notrials/**",
        "destination": "/rsl-no-trials-tracker.html"
      },
      {
        "source": "/default/**",
        "destination": "/rsl-s2-tracker.html"
      }
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ]
  }
}
